<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.sla.TwoWayCommunicationController">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <VBox prefHeight="657.0" prefWidth="504.0">
         <children>
            <HBox prefHeight="33.0" prefWidth="341.0">
               <children>
                  <Label prefHeight="17.0" prefWidth="86.0" text="IP Address:" />
                  <TextField fx:id="IPAddressText" prefHeight="27.0" prefWidth="430.0" />
               </children>
            </HBox>
            <HBox prefHeight="33.0" prefWidth="341.0">
               <children>
                  <Label prefHeight="17.0" prefWidth="86.0" text="Port:" textAlignment="RIGHT" />
                  <TextField fx:id="portText" prefHeight="27.0" prefWidth="344.0" />
                  <Button fx:id="startButton" mnemonicParsing="false" onAction="#startButtonPressed" prefHeight="27.0" prefWidth="97.0" text="Start" />
               </children>
            </HBox>
            <HBox prefHeight="32.0" prefWidth="341.0">
               <children>
                  <Label prefHeight="17.0" prefWidth="79.0" text="Status: " />
                  <TextField fx:id="statusText" prefHeight="27.0" prefWidth="424.0" />
               </children>
            </HBox>
            <TextField fx:id="sendText" prefHeight="251.0" prefWidth="504.0" />
            <Button fx:id="sendButton" mnemonicParsing="false" onAction="#sendButtonPressed" prefHeight="27.0" prefWidth="507.0" text="Send" />
            <TextField fx:id="receiveText" prefHeight="292.0" prefWidth="504.0" />
         </children>
      </VBox>
   </children>
</GridPane>
